{
	"_name": <%= name + '-root' %>,
	"transform": {
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>,
		"scale": [1, 1.25, 1]
	},
	"renderable": "door-frame-1.5x2",
	"physics": {
		"model": "door-frame-1.5x2",
		"dynamic": false,
		"decomposeHull": true
	}
},
{
	"_name": <%= name + '-left' %>,
	"renderable": "door-panel-l-1.5x2",
	"transform": {
		"parent": <%= name + '-root' %>,
		"translate": [0, 0, 0]
	},
	"physics": {
		"model": "door-panel-l-1.5x2",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-right' %>,
	"renderable": "door-panel-r-1.5x2",
	"transform": {
		"parent": <%= name + '-root' %>,
		"translate": [0, 0, 0]
	},
	"physics": {
		"model": "door-panel-r-1.5x2",
		"dynamic": false
	}
},
{
	"_name": <%= name %>,
	"transform": {
		"parent": <%= name + '-root' %>
	},
	"slideDoor": {
		"left": <%= name + '-left' %>,
		"right": <%= name + '-right' %>,
		"width": 1.3,
		"openTime": 0.5,
		"forward": [1, 0, 0]
	}
}