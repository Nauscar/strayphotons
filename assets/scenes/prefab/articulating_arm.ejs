        "angular_damping": 100,
        "linear_damping": 100,
        "model": {
            "model": "articulating-arm",
            "mesh_index": 3
        },
        "joint": {
            "target": <%= name + ".Arm_1" %>,
            "type": "spherical",
            "range": [90, 90],
            "local_offset": {
                "rotate": [90, 0, 0, 1]
            },
            "remote_offset": {
                "translate": [0, 0.1043, 0],
