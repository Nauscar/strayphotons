{
	"_name": <%= name %>,
	"renderable": "airlock",
	"transform": {
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>
	},
	"physics": {
		"model": "airlock",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-out-left' %>,
	"renderable": "door-panel-l-1.5x2",
	"transform": {
		"parent": <%= name %>,
		"translate": [-0.05, 0, 0],
		"scale": [2, 1.25, 1]
	},
	"physics": {
		"model": "door-panel-l-1.5x2",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-out-right' %>,
	"renderable": "door-panel-r-1.5x2",
	"transform": {
		"parent": <%= name %>,
		"translate": [-0.05, 0, 0],
		"scale": [2, 1.25, 1]
	},
	"physics": {
		"model": "door-panel-r-1.5x2",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-out' %>,
	"slideDoor": {
		"left": <%= name + '-out-left' %>,
		"right": <%= name + '-out-right' %>,
		"width": 1.3,
		"openTime": 0.5,
		"forward": [1, 0, 0]
	}
},
{
	"_name": <%= name + '-in-left' %>,
	"renderable": "door-panel-l-1.5x2",
	"transform": {
		"parent": <%= name %>,
		"translate": [2.75, 0, 0],
		"scale": [2, 1.25, 1]
	},
	"physics": {
		"model": "door-panel-l-1.5x2",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-in-right' %>,
	"renderable": "door-panel-r-1.5x2",
	"transform": {
		"parent": <%= name %>,
		"translate": [2.75, 0, 0],
		"scale": [2, 1.25, 1]
	},
	"physics": {
		"model": "door-panel-r-1.5x2",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-in' %>,
	"slideDoor": {
		"left": <%= name + '-in-left' %>,
		"right": <%= name + '-in-right' %>,
		"width": 1.3,
		"openTime": 0.5,
		"forward": [1, 0, 0]
	}
},
{
	"light": {
		"tint": [0.5, 0.5, 1],
		"intensity": 10,
		"spotAngle": 60
	},
	"view": {
		"extents": [2048, 2048],
		"clip": [0.1, 70]
	},
	"transform": {
		"parent": <%= name %>,
		"rotate": [-90, 1, 0, 0],
		"translate": [1.4, 2.5, 0.8]
	}
}
