{
	"_name": <%= name + '-root' %>,
	"transform": {
		<% if (locals.parent) { %>"parent": <%= parent %>,<% } %>
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>,
		"scale": [1, 1, 1]
	},
	"renderable": "door-frame",
	"physics": {
		"model": "door-frame",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-left' %>,
	"renderable": "door-panel",
	"transform": {
		"parent": <%= name + '-root' %>
	},
	"physics": {
		"model": "door-panel",
		"dynamic": false
	}
},
{
	"_name": <%= name + '-right' %>,
	"renderable": "door-panel",
	"transform": {
		"parent": <%= name + '-root' %>,
		"rotate": [180, 0, 1, 0]
	},
	"physics": {
		"model": "door-panel",
		"dynamic": false
	}
},
{
	"_name": <%= name %>,
	"slideDoor": {
		"left": <%= name + '-left' %>,
		"right": <%= name + '-right' %>,
		"width": 1.25,
		"openTime": 0.5,
		"forward": [1, 0, 0]
	}
}
