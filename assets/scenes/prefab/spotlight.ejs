{
	"_name": <%= name %>,
	"renderable": "spotlight-01",
	"light": {
		"tint": <%= tint %>,
		"intensity": <%= intensity %>,
		"spotAngle": <%= spotAngle %> 
	},
	"view": {
		"extents": [2048, 2048],
		"clip": [0.1, 70]
	},
	"transform": {
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>
	}
},
{
	"renderable": {
		"model": "box",
		"emissive": <%= intensity / 10 %>
	},
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 0, -0.06],
		"scale": [0.1, 0.1, 0.03]
	}
}
