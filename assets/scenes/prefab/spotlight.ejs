{
	"_name": <%= name %>,
	"renderable": "spotlight-01",
	"light": {
		<% if (locals.on != null) { %>"on": <%= on %>,<% } %>
		"tint": <%= tint %>,
		"intensity": <%= intensity %>,
		"spotAngle": <%= spotAngle %> 
	},
	"view": {
		"extents": [2048, 2048],
		"clip": [0.1, 70]
	},
	<% if (locals.physics) { %>
	"physics": {
		"model": "spotlight-01",
		"dynamic": false
	},
	<% } %>
	"transform": {
		<% if (locals.rotate) { %>"rotate": <%= rotate %>,<% } %>
		"translate": <%= translate %>
	}
},
{
	"renderable": "box",
	"bulb": <%= name %>,
	"transform": {
		"parent": <%= name %>,
		"translate": [0, 0, -0.06],
		"scale": [0.1, 0.1, 0.03]
	}
}
